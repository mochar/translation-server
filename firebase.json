{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/web",
        "function": "api"
      },
      {
        "source": "/search",
        "function": "api"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "functions": {
    "source": "."
  }
}
